SELECT `user_dialog`.peer_id  `dialogs`.title, `dialogs`.`description`, dialogs.adminUsers,dialogs.creator
FROM `user_dialog` LEFT JOIN `dialogs`
ON `user_dialog`.`dialog_id`= `dialogs`.`id`
WHERE `user_dialog`.`user_id` = 345358434